@article{Majka2012,
  title = {{{ATP}} Hydrolysis by {{RAD50}} Protein Switches {{MRE11}} Enzyme from Endonuclease to Exonuclease},
  author = {Majka, Jerzy and Alford, Brian and Ausio, Juan and Finn, Ron M. and McMurray, Cynthia T.},
  year = {2012},
  journal = {Journal of Biological Chemistry},
  volume = {287},
  number = {4},
  pages = {2328--2341},
  issn = {00219258},
  doi = {10.1074/jbc.M111.307041},
  abstract = {MRE11-RAD50 is a key early response protein for processing DNA ends of broken chromosomes for repair, yet how RAD50 nucleotide dynamics regulate MRE11 nuclease activity is poorly understood. We report here that ATP binding and ATP hydrolysis cause a striking butterfly-like opening and closing of the RAD50 subunits, and each structural state has a dramatic functional effect on MRE11. RAD50-MRE11 has an extended conformation in solution when MRE11 is an active nuclease. However, ATP binding to RAD50 induces a closed conformation, and in this state MRE11 is an endonuclease. ATP hydrolysis opens the RAD50-MRE11 complex, and MRE11 maintains exonuclease activity. Thus, ATP hydrolysis is a molecular switch that converts MRE11 from an endonuclease to an exonuclease. We propose a testable model in which the open-closed transitions are used by RAD50-MRE11 to discriminate among DNA ends and drive the choice of recombination pathways.},
  pmid = {22102415},
  file = {C\:\\Users\\makro\\Zotero\\storage\\BD7CC3ZF\\Majka et al. - 2012 - ATP hydrolysis by RAD50 protein switches MRE11 enzyme from endonuclease to exonuclease.pdf}
}

@article{Marchioni2018,
  title = {Silver Nanoparticle Fate in Mammals: {{Bridging}} {\emph{in Vitro}} and {\emph{in Vivo }}Studies},
