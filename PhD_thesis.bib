@article{Mari2006,
  title = {Dynamic Assembly of End-Joining Complexes Requires Interaction between {{Ku70}}/80 and {{XRCC4}}},
  author = {Mari, Pierre Olivier and Florea, Bogdan I. and Persengiev, Stephan P. and Verkaik, Nicole S. and Br{\"u}ggenwirth, Hennie T. and Modesti, Mauro and {Giglia-Mari}, Giuseppina and Bezstarosti, Karel and Demmers, Jeroen A.A. and Luider, Theo M. and Houtsmuller, Adriaan B. and Van Gent, Dik C.},
