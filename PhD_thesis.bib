@article{Tran2022,
  title = {Relations between {{Structure}} and {{Zn}}({{II}}) {{Binding Affinity Shed Light}} on the {{Mechanisms}} of {{Rad50 Hook Domain Functioning}} and {{Its Phosphorylation}}},
