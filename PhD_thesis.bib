  number = {6},
  pages = {440--446},
  issn = {0340-5761},
  doi = {10.1007/BF00288347},
  abstract = {The effects of mercuric chloride on cell survival, phagocytosis and cell migration were examined in cultured mouse peritoneal macrophages, and the accumulation of mercuric chloride in the cells was visualized by autometallography and evaluated by light and electron microscopy. Macrophages exposed to mercury concentrations from 1.25 {$\mu$}M to 10 {$\mu$}M mercuric chloride showed a concentration- and time-dependent increase in mercuric chloride accumulation, while cells exposed to 20 {$\mu$}M and 40 {$\mu$}M mercury showed an inverse relationship between mercury concentration and the accumulation of mercury. Mercury concentrations above these levels caused cell necrosis. Electron microscopy revealed that mercury was located primarily within lysosomes but also in the nucleus and cytoplasm. Mercury increased the death rate of macrophages in a concentration-dependent manner when cells were treated with mercury concentrations not causing cell necrosis. Further, we found that mercury clearly impaired macrophage random migration and possibly the capability for phagocytosis. \textcopyright{} 1988 Springer-Verlag.},
  pmid = {3250374},
  keywords = {Autometallography,Macrophages,Mercuric chloride,Migration,Phagocytosis,Toxicity},
  file = {C\:\\Users\\makro\\Zotero\\storage\\KUX2YRDY\\Christensen, Mogensen, Rungby - 1988 - Toxicity and ultrastructural localization of mercuric chloride in cultured murine macrophages.pdf}
}

