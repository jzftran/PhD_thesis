  file = {C\:\\Users\\makro\\Zotero\\storage\\PU4B2V5Z\\Hopfner et al. - 2000 - Mre11 and Rad50 from Pyrococcus furiosus Cloning and biochemical characterization reveal an evolutionarily conse.pdf}
}

